{
  "app": {
    "name": "antigravity-project",
    "version": "1.0.0",
    "environment": "development",
    "debug": true
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 4
  },
  "database": {
    "driver": "postgresql",
    "host": "localhost",
    "port": 5432,
    "name": "app_db",
    "user": "app_user",
    "password": "${DB_PASSWORD}"
  },
  "logging": {
    "level": "INFO",
    "format": "json",
    "output": "stdout"
  },
  "features": {
    "caching": true,
    "rate_limiting": true,
    "cors_enabled": true
  }
}
